    "build:webapp:dev": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build webapp --prod --configuration=development",
    "build:webapp:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build webapp --prod",
    "deploy:webapp:dev": "./deploy-webapp-dev.sh",
    "deploy:webapp:prod": "./deploy-webapp-prod.sh"
